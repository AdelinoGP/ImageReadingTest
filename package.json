{
  "name": "imagereadingtest",
  "version": "1.0.0",
  "description": "Backend of an image reading back-end service, integrating NodeJS, Docker, Prisma and GeminiAI",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "test": "dotenv -e .env.test jest",
    "test-reset": "dotenv -e .env.test prisma migrate reset && dotenv -e .env.test jest",
    "migrate": "prisma migrate deploy && prisma generate",
    "dev": "ts-node-dev --inspect=0.0.0.0:9222 --respawn --transpile-only --exit-child --ignore-watch node_modules -r tsconfig-paths/register --watch src/ src/index.ts",
    "update-dev": "npm install --dev && npm run migrate",
    "start-dev": "npm run update-dev && npm run dev",
    "build": "tsc --build"
  },
